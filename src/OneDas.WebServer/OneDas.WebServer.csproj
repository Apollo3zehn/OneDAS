<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <ApplicationIcon>$(ProjectDir)Resources\NetworkIcon.ico</ApplicationIcon>
    <TargetFramework>net47</TargetFramework>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="BuildBundlerMinifier" Version="2.6.362" />
    <PackageReference Include="Microsoft.AspNetCore" Version="2.1.0-preview2-30258" />
    <PackageReference Include="Microsoft.AspNetCore.Hosting.WindowsServices" Version="2.1.0-preview2-30258" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.1.0-preview2-30258" />
    <PackageReference Include="Microsoft.AspNetCore.ResponseCompression" Version="2.1.0-preview2-30258" />
    <PackageReference Include="Microsoft.AspNetCore.StaticFiles" Version="2.1.0-preview2-30258" />
    <PackageReference Include="Microsoft.AspNetCore.SignalR" Version="1.0.0-preview2-30258" />
    <PackageReference Include="Microsoft.AspNetCore.SignalR.Client" Version="1.0.0-preview2-30258" />
    <PackageReference Include="Microsoft.Extensions.Logging.EventLog" Version="2.1.0-preview2-30258" />
    <PackageReference Include="NuGet.PackageManagement" Version="4.7.0-preview1-4986" />
    <PackageReference Include="OneDas.Engine" Version="1.0.0-alpha-6663-28138" />
    <PackageReference Include="System.ServiceProcess.ServiceController" Version="4.5.0-preview1-26216-02" />
  </ItemGroup>
  
  <ItemGroup>
    <DotNetCliToolReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Tools" Version="1.0.1" />
  </ItemGroup>
  
  <ItemGroup>
    <Compile Update="Resources\ErrorMessage.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>ErrorMessage.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  
  <ItemGroup>
    <EmbeddedResource Update="Resources\ErrorMessage.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <CustomToolNamespace>OneDas.WebServer</CustomToolNamespace>
      <LastGenOutput>ErrorMessage.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <Content Update="**\tsconfig.json" CopyToPublishDirectory="Never" />
  </ItemGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\OneDas.WebClient\OneDas.WebClient.csproj">
      <ReferenceOutputAssembly>false</ReferenceOutputAssembly>
    </ProjectReference>
  </ItemGroup>
  
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>portable</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>
  
  <ProjectExtensions><VisualStudio><UserProperties Properties_4launchSettings_1json__JSONSchema="" /></VisualStudio></ProjectExtensions>
  
</Project>
