<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <Description>Provides a web-based user interface to browse the OneDAS database and export desired data.</Description>
    <PackageTags>OneDAS explorer export</PackageTags>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.NETCore.Targets" Version="3.1.0" />
    <PackageReference Include="BlazorInputFile" Version="0.1.0-preview-00002" />
    <PackageReference Include="ChartJs.Blazor" Version="1.0.4-preview1" />
    <PackageReference Include="MatBlazor" Version="2.2.0" />
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="3.1.2" />
    <PackageReference Include="Microsoft.AspNetCore.Identity.EntityFrameworkCore" Version="3.1.2" />
    <PackageReference Include="Microsoft.AspNetCore.Identity.UI" Version="3.1.2" />
    <PackageReference Include="Microsoft.AspNetCore.SignalR.Protocols.MessagePack" Version="3.1.2" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="3.1.2" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="3.1.2" />
    <PackageReference Include="Microsoft.Extensions.Hosting.WindowsServices" Version="3.1.2" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="3.1.2" />
    <PackageReference Include="Prism.Core" Version="7.2.0.1422" />
    <PackageReference Include="Serilog.Extensions.Logging.File" Version="2.0.0" />
    <PackageReference Include="System.CommandLine" Version="2.0.0-beta1.20104.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\extensions\OneDas.Extension.Csv\OneDas.Extension.Csv.csproj" />
    <ProjectReference Include="..\..\extensions\OneDas.Extension.Famos\OneDas.Extension.Famos.csproj" />
    <ProjectReference Include="..\..\extensions\OneDas.Extension.Mat73\OneDas.Extension.Mat73.csproj" />
    <ProjectReference Include="..\OneDas.DataManagement.Core\OneDas.DataManagement.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resources\EnumerationDescription.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>EnumerationDescription.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resources\EnumerationDescription.resx">
      <CustomToolNamespace>OneDas.DataManagement.Explorer</CustomToolNamespace>
      <LastGenOutput>EnumerationDescription.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
  </ItemGroup>

</Project>
